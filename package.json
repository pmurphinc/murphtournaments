{
  "name": "murph-tournaments-ops",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "type-check": "tsc --noEmit",
    "prebuild": "node scripts/embed-png-as-svg.mjs public/discord-app-icon.png public/logo.svg 128",
    "sync:logo": "node scripts/embed-png-as-svg.mjs public/discord-app-icon.png public/logo.svg 128",
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "lint": "next lint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "e2e": "playwright test",
    "format": "prettier --write ."
  },
  "engines": {
    "node": ">=20.10.0"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@supabase/supabase-js": "^2.45.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "discord.js": "^14.16.3",
    "next": "14.2.11",
    "next-auth": "5.0.0-beta.29",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recharts": "^2.12.7",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.2",
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.11",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prisma": "5.22.0",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.2"
  },
  "pnpm": {
    "overrides": {
      "eslint": "8.57.0"
    }
  }
}
